{
  "name": "trpc-openapi-api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "prepare": "husky install && npm run setup:env",
    "setup:env": "cp .env.example .env",
    "start": "ts-node ./src/index.ts",
    "dev": "dotenv node-dev ./src/index.ts",
    "test": "dotenv -e .env.test -- vitest",
    "test:ui": "dotenv -e .env.test -- vitest --ui",
    "coverage": "dotenv -e .env.test -- vitest run --coverage",
    "lint": "rome check ./src/",
    "format": "rome format ./src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "npm run format -- --write"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/swagger": "^8.5.1",
    "@fastify/swagger-ui": "^1.8.1",
    "@swc/core": "^1.3.62",
    "@trpc/server": "^10.29.1",
    "dd-trace": "^4.1.1",
    "fastify": "^4.17.0",
    "kysely": "^0.25.0",
    "mysql2": "^3.3.3",
    "prisma-kysely": "^1.4.2",
    "trpc-openapi": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/uuid": "^9.0.2",
    "@vitest/coverage-v8": "^0.32.0",
    "@vitest/ui": "^0.32.0",
    "dotenv-cli": "^7.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "node-dev": "^8.0.0",
    "prisma": "4.15.0",
    "rome": "12.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  }
}
